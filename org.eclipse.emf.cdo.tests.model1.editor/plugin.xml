<plugin>

  <extension
    point = "org.eclipse.ui.newWizards">
    <category
      id = "org.eclipse.emf.ecore.Wizard.category.ID"
      name="%_UI_Wizard_category">
    </category>
    <wizard
      id = "org.eclipse.emf.cdo.tests.model1.presentation.Model1ModelWizardID"
      name = "%_UI_Model1ModelWizard_label"
      class = "org.eclipse.emf.cdo.tests.model1.presentation.Model1ModelWizard"
      category = "org.eclipse.emf.ecore.Wizard.category.ID"
      icon = "icons/full/obj16/Model1ModelFile.gif">
      <description>%_UI_Model1ModelWizard_description</description>
      <selection class = "org.eclipse.core.resources.IResource" />
    </wizard>
  </extension>

  <extension point = "org.eclipse.ui.editors">
    <editor
        id = "org.eclipse.emf.cdo.tests.model1.presentation.Model1EditorID"
        name = "%_UI_Model1Editor_label"
        icon = "icons/full/obj16/Model1ModelFile.gif"
        extensions = "model1"
        class = "org.eclipse.emf.cdo.tests.model1.presentation.Model1Editor" 
        contributorClass="org.eclipse.emf.cdo.tests.model1.presentation.Model1ActionBarContributor" >
    </editor>
  </extension>    

</plugin>
