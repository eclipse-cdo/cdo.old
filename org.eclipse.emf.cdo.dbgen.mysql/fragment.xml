<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<fragment>

   <extension
         point="org.eclipse.emf.cdo.dbgen.dialects">
      <dialect
            createIndexTemplate="CREATE INDEX &quot;{2}_{1}&quot; ON &quot;{2}&quot; ({0})"
            createPrimaryIndexTemplate="ALTER TABLE &quot;{2}&quot; ADD (CONSTRAINT &quot;{2}_{1}&quot; PRIMARY KEY (&quot;{0}&quot;))"
            createTablePrimaryTemplate="CREATE TABLE &quot;{1}&quot; ({0}, PRIMARY KEY (&quot;{2}&quot;)) TYPE=INNODB"
            createTableTemplate="CREATE TABLE &quot;{1}&quot; ({0}) TYPE=INNODB"
            createUniqueIndexTemplate="CREATE UNIQUE INDEX &quot;{2}_{1}&quot; ON &quot;{2}&quot; ({0})"
            dropTableTemplate="DROP TABLE IF EXISTS &quot;{0}&quot;"
            name="MYSQL">
         <typeMapping
               sqlType="BIT"
               vendorString="BIT"/>
         <typeMapping
               sqlType="BOOLEAN"
               vendorString="BIT"/>
         <typeMapping
               sqlType="DOUBLE"
               vendorString="DOUBLE"/>
         <typeMapping
               sqlType="INTEGER"
               vendorString="INT"/>
         <typeMapping
               sqlType="LONGVARBINARY"
               vendorString="LONG VARBINARY"/>
         <typeMapping
               sqlType="LONGVARCHAR"
               vendorString="LONG VARCHAR"/>
         <typeMapping
               sqlType="TIMESTAMP"
               vendorString="TIMESTAMP"/>
         <typeMapping
               sqlType="VARCHAR"
               vendorString="VARCHAR"/>
      </dialect>
   </extension>

</fragment>
