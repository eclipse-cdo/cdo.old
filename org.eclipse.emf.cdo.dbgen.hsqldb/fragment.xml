<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<fragment>

   <extension
         point="org.eclipse.emf.cdo.dbgen.dialects">
      <dialect
            createIndexTemplate="CREATE INDEX &quot;{2}_{1}&quot; ON &quot;{2}&quot; ({0})"
            createPrimaryIndexTemplate="ALTER TABLE &quot;{2}&quot; ADD (CONSTRAINT &quot;{2}_{1}&quot; PRIMARY KEY (&quot;{0}&quot;))"
            createTablePrimaryTemplate="CREATE TABLE &quot;{1}&quot; ({0}, CONSTRAINT &quot;{2}_{1}&quot; PRIMARY KEY (&quot;{2}&quot;))"
            createTableTemplate="CREATE TABLE &quot;{1}&quot; ({0})"
            createUniqueIndexTemplate="CREATE UNIQUE INDEX &quot;{2}_{1}&quot; ON &quot;{2}&quot; ({0})"
            dropTableTemplate="DROP TABLE &quot;{0}&quot;"
            name="HSQLDB">
         <typeMapping
               sqlType="LONGVARBINARY"
               vendorString="LONGVARBINARY"/>
         <typeMapping
               sqlType="LONGVARCHAR"
               vendorString="LONGVARCHAR"/>
      </dialect>
   </extension>

</fragment>
