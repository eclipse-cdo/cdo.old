<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<fragment>

     <extension
         name="CDO Pluggable HSQLDB Driver"
         point="org.eclipse.emf.cdo.jdbc.drivers">
      <driver
            exampleURL="jdbc:hsqldb:file:&lt;database&gt;"
            className="org.hsqldb.jdbcDriver"
            name="HSqlDB">
         <supportedDialect name="HSQLDB"/>
      </driver>
   </extension>

</fragment>
