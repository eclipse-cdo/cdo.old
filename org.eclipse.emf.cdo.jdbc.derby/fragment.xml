<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<fragment>

     <extension
         name="CDO Pluggable DERBY Driver"
         point="org.eclipse.emf.cdo.jdbc.drivers">
      <driver
            exampleURL="jdbc:derby://&lt;server&gt;[:&lt;port&gt;]/databaseName[;URLAttributes=&lt;value&gt; [;...]]"
            className="org.apache.derby.jdbc.EmbeddedDriver"
            name="Derby">
         <supportedDialect name="ANSI92"/>
         <library
               downloadURL="http://db.apache.org/derby/derby_downloads.html"
               entryPath="lib/derby.jar"
               fragmentId="org.eclipse.emf.cdo.jdbc.derby"/>
      </driver>
   </extension>

</fragment>
