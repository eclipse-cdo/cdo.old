<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<fragment>

     <extension
         name="CDO Pluggable MYSQL Driver"
         point="org.eclipse.emf.cdo.jdbc.drivers">
      <driver
            exampleURL="jdbc:mysql://&lt;host&gt;/&lt;database&gt;"
            className="org.gjt.mm.mysql.Driver"
            name="MySql">
         <supportedDialect name="MYSQL"/>
         <library
               downloadURL="http://dev.mysql.com/downloads/connector/j/3.1.html"
               entryPath="lib/mysql-connector-java-3.1.12-bin.jar"
               fragmentId="org.eclipse.emf.cdo.jdbc.mysql"/>
      </driver>
   </extension>

</fragment>
